---
import Document from "../../components/layout/Document/Document.astro";
import Layout from "../../components/layout/Layout/Layout.astro";
import CharityPage from "../../components/page/Charity/CharityPage.astro";
import type { BreadcrumbItems } from "../../components/ui/Breadcrumb/type";

const title = "尼崎のこども食堂への支援";

const breadcrumb: BreadcrumbItems = [
  {
    title: "トップページ",
    href: "/",
  },
  {
    title,
  },
];
---

<Document
  title={title}
  desc="尼崎のこども食堂を支援するための方法を記載しています。多くの食堂が地域のこどもたちにとっての「身近な地域の居場所」であり続けられるために、応援よろしくお願いします。"
>
  <Layout breadcrumb={breadcrumb}>
    <CharityPage title={title} />
  </Layout>
</Document>
