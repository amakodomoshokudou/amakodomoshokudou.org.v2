---
import type { BreadcrumbItems } from "../../components/ui/Breadcrumb/type";
import Document from "../../components/layout/Document/Document.astro";
import Layout from "../../components/layout/Layout/Layout.astro";
import AboutPage from "../../components/page/About/AboutPage.astro";

const title = "私たちについて";

const breadcrumb: BreadcrumbItems = [
  {
    title: "トップページ",
    href: "/",
  },
  {
    title,
  },
];
---

<Document
  title={title}
  desc="尼崎こども食堂ネットワークは尼崎市内のこども食堂が、これからもアマのこどもたちにとっての「身近な地域の居場所」であり続けられるように活動している団体です。"
>
  <Layout breadcrumb={breadcrumb}>
    <AboutPage title={title} />
  </Layout>
</Document>
