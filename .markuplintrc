{
  "extends": [
    "markuplint:recommended"
  ],
  "parser": {
    ".astro$": "@markuplint/astro-parser"
  },
  "pretenders": [
    {
      "selector": "Image",
      "as": "img"
    }
  ],
  "rules": {
    "character-reference": false
  },
  "nodeRules": [
    {
      "selector": "img",
      "rules": {
        "required-attr": false
      }
    }
  ]
}
