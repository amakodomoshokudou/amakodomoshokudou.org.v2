{
  "extends": [
    "markuplint:recommended"
  ],
  "nodeRules": [
    {
      "selector": "link",
      "rules": {
        "invalid-attr": {
          "options": {
            "allowAttrs": [
              {
                "name": "rel",
                "value": {
                  "enum": [
                    "stylesheet",
                    "preload",
                    "icon",
                    "apple-touch-icon",
                    "canonical",
                    "sitemap"
                  ]
                }
              }
            ]
          }
        }
      }
    }
  ]
}
